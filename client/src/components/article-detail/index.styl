@import '../../styl/variable.styl'
.wrapper
  flex-grow 1
  margin-right 20px
  box-shadow $box-shadow-light
  padding 15px 35px
  header
    border-bottom 1px solid $border
    padding-bottom 10px
  .title
    padding 7px 0
    font-size 20px
    font-weight 700
    color rgba(0,0,0,0.85)
    line-height $line-height
  .info
    color $greyFont
    line-height $line-height
    font-size 14px
